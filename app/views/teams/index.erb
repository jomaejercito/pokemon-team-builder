<h1>Welcome, <%= @current_user.username %></h1>
<br>

  <% Team.all.each do |team| %>
    <h2>Team Name: <%= team.name %></h2>
    <h3>Trainer: <%= team.user.username %></h3>
    <a href='/teams/<%= team.id %>'>View Team</a>
    <br><br>
  <% end %>

<br><br><br><br><br>

<ul>
  <li><a href="/teams/new"> Create a Team </a></li>
  <li><a href="/teams/show"> View Other Teams </a></li>
  <li><a href="/logout"> Logout </a></li>
</ul>
